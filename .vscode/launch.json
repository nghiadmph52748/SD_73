{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "BeSpApplication",
      "request": "launch",
      "mainClass": "org.example.be_sp.BeSpApplication",
      "projectName": "BE_SP"
    },
    {
      "name": "Launch Chrome",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}/DATN_GearUp_Ver2/src",
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/*"
      }
    },
    {
      "name": "Attach to Chrome",
      "type": "chrome",
      "request": "attach",
      "port": 9222,
      "webRoot": "${workspaceFolder}/DATN_GearUp_Ver2/src"
    }
  ]
}
